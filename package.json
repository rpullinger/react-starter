{
  "scripts": {
    "preclean": "mkdir -p build",
    "clean": "rm -r build",
    "postclean": "npm run preclean",
    "test": "NODE_ENV=production mocha-webpack --webpack-config webpack.config.js 'app/**/*.test.js' --colors --reporter list",
    "watch:test": "npm run test -- --watch",
    "build:css": "postcss -c postcss.config.js",
    "postbuild:css": "npm run minify:css",
    "watch:css": "postcss -c postcss.config.js -w",
    "minify:css": "postcss -u cssnano build/base.css -o build/base.min.css",
    "build:js": "webpack -p",
    "watch:js": "webpack --progress --colors --watch",
    "dev:server": "parallelshell 'webpack-dev-server --progress --colors --hot --inline --content-base build/' 'browser-sync start --files 'build/base.css' --proxy http://localhost:8080/'",
    "predev:server": "npm run build:static",
    "build:static": "cp -r public/* build/",
    "watch:static": "chokidar 'public/**/*' -c 'npm run build:static'",
    "storybook": "start-storybook -p 9001",
    "prewatch": "npm run clean",
    "watch": "parallelshell 'npm run dev:server' 'npm run watch:static' 'npm run watch:test' 'npm run watch:css' 'npm run storybook'",
    "prebuild": "npm run clean",
    "build": "parallelshell 'npm run build:static' 'npm run build:js' 'npm run build:css'"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.5.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "cssnano": "^3.5.2",
    "mocha": "^2.4.5",
    "mocha-webpack": "^0.3.0",
    "parallelshell": "^2.0.0",
    "postcss": "^5.0.19",
    "postcss-calc": "^5.2.0",
    "postcss-cli": "^2.5.1",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^8.1.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "raw-loader": "^0.5.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "redux": "^3.3.1",
    "redux-devtools": "^3.2.0",
    "redux-responsive": "^1.1.0",
    "redux-thunk": "^2.0.1",
    "style-loader": "^0.13.1"
  }
}
